SYSNAME=`uname`
OBJTYPE=`uname -m | sed 's;i.86;386;; s;/.*;;; s; ;;g'`

X11=/usr/X11R6
LDFLAGS=
CFLAGS=
AFLAGS=

BIN=$PLAN9/bin
LIBDIR=$PLAN9/lib

<|sed -E '"s/(.*)\+=/\1=$\1 /g"' $PLAN9/src/Make.$SYSNAME-$OBJTYPE

OS=$O
LD=9l
LDFLAGS=$LDFLAGS -L$LIBDIR

CLEANFILES=*.$O
NUKEFILES=

INSTALL=install -c
