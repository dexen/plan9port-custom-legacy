SYSNAME=`uname`
OBJTYPE=`uname -m | sed 's;i.86;386;; s;/.*;;; s; ;;g'`

X11=/usr/X11R6
CC=9c
LD=9l
AS=9a
CFLAGS=
LDFLAGS=
AFLAGS=
O=o

BIN=$PLAN9/bin
LIBDIR=$PLAN9/lib

<|cat $PLAN9/src/mk.$SYSNAME-$OBJTYPE 2>/dev/null || true

OS=$O
LD=9l
LDFLAGS=$LDFLAGS -L$LIBDIR

CLEANFILES=*.$O
NUKEFILES=

INSTALL=install -c
